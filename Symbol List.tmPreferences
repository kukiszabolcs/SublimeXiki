<plist version="1.0">
<dict>
  <key>name</key>
  <string>Symbol List</string>

  <key>scope</key>
  <string>match.commands, match.folders, keyword.control.header.xiki</string>
  
  <key>settings</key>
  <dict>
    <key>showInSymbolList</key>
    <integer>1</integer>
    
  	<!-- Make sure that not more then 110 characters are sent per line to the method list -->
    <key>symbolTransformation</key>
    <string>s/^\s*(.{1,120}).*/$1/</string>
  </dict>
  
</dict>
